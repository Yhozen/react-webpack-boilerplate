{
  "name": "YARB",
  "version": "0.6.0",
  "description": "Yet Another React Boilerplate for react projects with ES2017 syntax and hot reloader using Webpack 3.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "lint": "eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty . *.js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yhozen/react-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "babel",
    "webpack"
  ],
  "author": "Garox (http://www.garox.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yhozen/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/yhozen/react-webpack-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^0.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^1.1.6",
    "json-loader": "^0.5.7",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^1.16.2"
  }
}
