{
  "presets": [ ["env", { "modules": false }], "stage-0", "react"],
  "env": {
      "development": {
          "plugins": [
            "transform-react-inline-elements",
            ["react-transform", {
              "transforms": [{
                "transform" : "react-transform-hmr",
                "imports"   : ["react"],
                "locals"    : ["module"]
          }]
        }]
      ]
    }
  }
}
